.base {
  display: flex;
  flex-direction: column;
  justify-content: center;
}

.base-row {
  display: flex;
  align-items: center;
}

.g4 {
  gap: 4px;
}

.g8 {
  gap: 8px;
}

.g16 {
  gap: 16px;
}

.jcsb {
  justify-content: space-between;
}

.aic {
  align-items: center;
}

.jcsa {
  justify-content: space-around;
}

.mt {
  padding-top: 4vh;
}

.plr-2 {
  padding-top: 1vh;
  padding-left: 2vw;
  padding-right: 2vw;
  border-bottom: solid .5px rgb(250, 250, 250);
}

.b {
  background-color:transparent;
  border: none;
  outline: none;
  padding: 0;
}

button::after {
  border: none;
}


.box-box {
  width: 100vw;
  height: 9vh;
  background-color:transparent;
}